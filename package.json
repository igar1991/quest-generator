{
  "name": "quest-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "jest",
    "type:check": "tsc --noEmit",
    "type:check:ci": "tsc --noEmit --project tsconfig.ci.json",
    "check:all": "npm run lint:check && npm run format:check && npm run type:check && npm run test",
    "check:all:ci": "npm run lint:check && npm run format:check && npm run type:check:ci && npm run test",
    "fix:all": "npm run lint:fix && npm run format",
    "redis:import": "node scripts/importQuests.js",
    "redis:clear": "node scripts/clearQuests.js"
  },
  "dependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "ioredis": "^5.6.0",
    "next": "15.2.2",
    "react": "^19.0.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
